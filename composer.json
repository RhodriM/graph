{
    "name": "RhodriM/Graph",
    "description": "A very simple implementation of a Graph data structure.",
    "type": "library",
    "keywords": ["graph", "network", "node", "edge", "gml"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rhodri Morris",
            "homepage": "http://rhodrimorris.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.30"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.7.*",
        "squizlabs/php_codesniffer" : "*",
        "phpmd/phpmd" : "@stable",
        "phpdocumentor/phpdocumentor": "2.*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "Graph\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graph\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
