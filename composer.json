    "require": {
        "php" : ">=5.6.30"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.7.*",
        "squizlabs/php_codesniffer" : "*",
        "phpmd/phpmd" : "@stable",
        "phpdocumentor/phpdocumentor": "2.*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
